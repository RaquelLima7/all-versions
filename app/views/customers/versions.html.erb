<div class="container mt-5">
  <p id="notice"><%= notice %></p>

  <h1 class="text-center m-5">Previous Versions</h1>

  <table class="table">
    <thead class='table-active th-text-color'>
      <tr>
        <th class="align-middle text-center">First name</th>
        <th class="align-middle text-center">Last name</th>
        <th class="align-middle text-center">Birthday</th>
        <th class="align-middle text-center">Country</th>
        <th class="align-middle text-center">Event</th>
        <th class="align-middle text-center">Date</th>
        <th colspan="7"></th>
      </tr>
    </thead>

    <tbody>
    <%= render partial: "customer", collection: @customers %>
    </tbody>
  </table>

  <br>

  <%= link_to 'Back', customers_path %>
</div>
